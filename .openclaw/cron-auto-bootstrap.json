{
  "name": "auto-bootstrap-on-interruption",
  "schedule": {
    "kind": "every",
    "everyMs": 300000
  },
  "payload": {
    "kind": "systemEvent",
    "text": "BOOTSTRAP_CHECK: Detect system interruption and trigger auto-continuity if needed"
  },
  "delivery": {
    "mode": "none"
  },
  "sessionTarget": "main",
  "enabled": false
}